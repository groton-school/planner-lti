    "@groton/canvas-api": "^0.3.4",
    "@groton/canvas-api.client.base": "^0.2.2",
    "@groton/canvas-api.client.web": "^0.2.6",
    "@groton/colors": "^0.4.2",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "change-case-all": "^2.1.0",
    "cookie": "^1.1.1",
    "ejs": "^3.1.10",
    "p-queue": "^8.1.1",
    "path-browserify": "^1.0.1",
    "tinycolor2": "^1.6.0"
  },
