{
  "version": "0.1.2",
  "description": "Provide Node-based deployment wizard to Google App Engine",
  "type": "module",
  "scripts": {
    "build": "source .env; op run --account $OP_ACCOUNT --no-masking --env-file ./.env -- webpack",
    "deploy": "run-s deploy:*",
    "deploy:build": "run-s build",
    "deploy:ui-template": "move-file ./public/index.html ./views/slim/UI.php",
    "deploy:gae": "source .env; ./scripts/deploy.js -v --serviceAccountToken \"$(op item get --account $OP_ACCOUNT --reveal --fields credential $OP_TOKEN)\"",
    "release": "commit-and-tag-version"
  },
  "dependencies": {
    "@fullcalendar/bootstrap5": "^6.1.19",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
