{
  "version": "0.1.1",
  "description": "Provide Node-based deployment wizard to Google App Engine",
  "type": "module",
  "scripts": {
    "build": "op run --no-masking --env-file ./.env -- webpack",
    "deploy": "run-s deploy:*",
    "deploy:build": "run-s build",
    "deploy:spa-template": "move-file ./public/index.html ./views/slim/SPA.php",
    "deploy:gae": "op run --no-masking --env-file ./.env -- node ./scripts/deploy.js -v",
    "release": "commit-and-tag-version"
  },
