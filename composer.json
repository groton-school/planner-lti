{
  "name": "groton-school/planner-lti",
  "description": "A more planner-oriented Canvas calendar view for students and teachers",
  "keywords": [
    "canvas",
    "google",
    "calendar",
    "planner",
    "todo",
    "lti"
  ],
  "homepage": "http://github.com/groton-school/planner-lti",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Seth Battis",
      "email": "sbattis@groton.org",
      "homepage": "https://www.groton.org"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "project",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "battis/lazy-secrets": "^1.1",
    "google/apiclient": "^2.18",
    "google/cloud-firestore": "^1.50",
    "groton-school/oauth2-canvaslms": "^1.1",
    "groton-school/slim-canvas-api-proxy": "^2.1",
    "groton-school/slim-gae-shim": "^2.1",
    "groton-school/slim-lti-infrastructure-gae": "^3.0",
    "groton-school/slim-lti-partitioned-session": "^2.2",
    "groton-school/slim-lti-shim": "^4.1",
    "groton-school/slim-norms": "^2.2",
    "groton-school/slim-oauth2-api-proxy": "^3.1",
    "groton-school/slim-oauth2-api-proxy-gae-repository": "^1.0",
    "guzzlehttp/guzzle": "^7.9",
    "league/oauth2-google": "^4.0",
    "monolog/monolog": "^2.9",
    "odan/session": "^6.3",
    "php-di/php-di": "^7.0",
    "psr/container": "^2.0",
    "psr/http-message": "^2.0",
    "psr/log": "^3.0",
    "slim/php-view": "^3.4",
    "slim/psr7": "^1.6",
    "slim/slim": "^4.12"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.75",
    "jangregor/phpstan-prophecy": "^1.0.0",
    "phpspec/prophecy-phpunit": "^2.2",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "^3.9"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public",
    "test": "phpunit",
    "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
  },
  "extra": {
    "google/apiclient-services": [
      "Calendar"
    ]
  }
}
